        <Motor name="front_left" port="1" />
        <Motor name="front_right" port="2" />
    </MotorController>
    <ServoController name="Servo Controller 1" serialNumber="AL00XLHS">
        <Servo name="Rightservo" port="1" />
        <Servo name="CatServo" port="2" />
